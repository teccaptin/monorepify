{
  "name": "monorepify",
  "version": "1.0.0",
  "description": "A boilerplate for Full stack VueJS and nestJS and other typescript frameworks too. But i believe you can add JS frameworks too (with little modification)",
  "private": true,
  "author": {
    "name": "Em Ji Madhu",
    "email": "emjimadhu@gmail.com"
  },
  "license": "MIT"
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:client:start": "cd apps/client && yarn serve",
    "dev:client:build": "cd apps/client && yarn build",
    "dev:client:lint": "cd apps/client && yarn lint",
    "test:client:unit": "cd apps/client && yarn test:unit",
    "test:client:e2e": "cd apps/client && yarn test:e2e"
  }
}
